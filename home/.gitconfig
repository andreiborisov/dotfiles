[core]
  editor = code --wait
[diff "plist"]
  textconv = plutil -convert xml1 -o -
[diff "scpt"]
  binary = true
  textconv = osadecompile
[init]
  defaultBranch = main
[merge]
  tool = smerge
[mergetool "smerge"]
  cmd = smerge mergetool \"$BASE\" \"$LOCAL\" \"$REMOTE\" -o \"$MERGED\"
  trustExitCode = true
